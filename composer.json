{
    "name": "kassko/fixture-project-banking",
    "type": "project",
    "description": "Symfony lightweight banking fixture project without Doctrine",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/yaml": "^7.0",
        "symfony/runtime": "^7.0",
        "nelmio/api-doc-bundle": "^4.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
