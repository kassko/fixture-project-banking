{
    "name": "kassko/fixture-project-banking",
    "description": "A comprehensive PHP/Symfony fixture project for testing object hydration in a banking/insurance domain",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.0",
        "symfony/orm-pack": "^2.4",
        "doctrine/orm": "^3.0",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "api-platform/core": "^3.2",
        "nelmio/api-doc-bundle": "^4.19",
        "symfony/asset": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/runtime": "^7.0"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.52"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
